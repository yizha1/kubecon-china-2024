{
    "version": "1.0",
    "trustPolicies": [
        {
            "name": "Acquire images from Bitnami",
            "registryScopes": [ "docker.io/bitnami/nginx", "docker.io/bitnami/fluentd" ],
            "signatureVerification": {
                "level" : "strict" 
            },
            "trustStores": [ "ca:bitnami"],
            "trustedIdentities": [
                "*"
            ]
        }
    ]
}
